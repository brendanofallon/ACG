<?xml version="1.0" encoding="UTF-8"?>
<ACG>
    <RandomSource class="math.RandomSource"/>
    <Alignment class="sequence.Alignment">
        <sequences1 class="list">
            <tip6 class="sequence.Sequence"></tip6>
            <tip9 class="sequence.Sequence"></tip9>
            <tip8 class="sequence.Sequence"></tip8>
            <tip1 class="sequence.Sequence"></tip1>
            <tip3 class="sequence.Sequence">AATAAGTATTCGTCAATTGACTGTAAAAGGAAGTCAACCCAGGTGCGTCACGGAAAATAG---</tip3>
            <tip5 class="sequence.Sequence">AATAAGTATTCGTCAATTGACTGTAAAAGGAAGTCAACCCAGGTGCGTCACGGAAAATAG---</tip5>
            <tip0 class="sequence.Sequence">---</tip0>
            <tip4 class="sequence.Sequence">----</tip4>
            <tip2 class="sequence.Sequence">------</tip2>
        </sequences1>
    </Alignment>
    <ARG class="arg.ARG" frequency="10.0">
        <Alignment/>
        <ARGModifiers class="list">
            <RecombAddRemove class="modifier.RecombAddRemove" frequency="5.0"/>
            <RootHeight class="modifier.RootHeightModifier" frequency="1.0"/>
            <NodeHeight class="modifier.NodeHeightModifier" frequency="25.0"/>
            <SubTreeSwapper class="modifier.SubtreeSwap" frequency="5.0"/>
            <WideSwapper class="modifier.WideSwap" frequency="5.0"/>
            <BPShifter class="modifier.BreakpointShifter" frequency="5.0"/>
            <BPSwapper class="modifier.BreakpointSwapper" frequency="5.0"/>
        </ARGModifiers>
    </ARG>
    <RateModel class="dlCalculation.siteRateModels.ConstantSiteRates" rate="1.0"/>
    <MutationModel class="dlCalculation.substitutionModels.F84Matrix">
        <BaseFrequencies
            class="dlCalculation.substitutionModels.BaseFrequencies" stationaries="0.25  0.25  0.25  0.25">
            <BaseFrequenciesMod class="modifier.DirichletModifier" frequency="0.1"/>
        </BaseFrequencies>
        <Kappa class="parameter.DoubleParameter" frequency="1.0"
            lowerBound="0.0" upperBound="Infinity" value="1.0">
            <KappaMod class="modifier.ScaleModifier" frequency="1.0"/>
        </Kappa>
    </MutationModel>
    <DataLikelihood class="dlCalculation.DataLikelihood">
        <MutationModel/>
        <RateModel/>
        <ARG/>
    </DataLikelihood>
    <PopulationSize class="coalescent.ExponentialGrowth">
        <BaseSize class="parameter.DoubleParameter" frequency="1.0"
            lowerBound="0.0" upperBound="Infinity" value="0.005">
            <BaseSizeModifier class="modifier.ScaleModifier" frequency="1.0"/>
        </BaseSize>
        <GrowthRate class="parameter.DoubleParameter" frequency="1.0"
            lowerBound="-Infinity" upperBound="Infinity" value="0.0">
            <GrowthRateModifier class="modifier.SimpleModifier" frequency="1.0"/>
        </GrowthRate>
    </PopulationSize>
    <RecombinationRate class="coalescent.ConstantRecombination"
        frequency="1.0" lowerBound="0.0" upperBound="Infinity" value="1.0">
        <RecombRateModifier class="modifier.ScaleModifier" frequency="0.1"/>
    </RecombinationRate>
    <CoalescentLikelihood class="coalescent.CoalescentLikelihood">
        <PopulationSize/>
        <RecombinationRate/>
        <ARG/>
    </CoalescentLikelihood>
    <KappaPrior class="priors.UniformPrior" lowerBound="0.0" upperBound="Infinity">
        <Kappa/>
    </KappaPrior>
    <BaseSizePrior class="priors.UniformPrior" lowerBound="0.0" upperBound="Infinity">
        <BaseSize/>
    </BaseSizePrior>
    <GrowthRatePrior class="priors.UniformPrior" lowerBound="-Infinity" upperBound="Infinity">
        <GrowthRate/>
    </GrowthRatePrior>
    <RecombinationRatePrior class="priors.UniformPrior" lowerBound="0.0" upperBound="Infinity">
        <RecombinationRate/>
    </RecombinationRatePrior>
    <StateLogger class="logging.StateLogger" echoToScreen="false"
        filename="StateLogger.log" frequency="10000"/>
    <MCMC class="mcmc.MCMC" length="10000000" run="false">
        <MCParams class="list">
            <ARG/>
            <Kappa/>
            <BaseFrequencies/>
            <GrowthRate/>
            <BaseSize/>
            <RecombinationRate/>
        </MCParams>
        <MCLikelihoods class="list">
            <DataLikelihood/>
            <CoalescentLikelihood/>
            <KappaPrior/>
            <BaseSizePrior/>
            <GrowthRatePrior/>
            <RecombinationRatePrior/>
        </MCLikelihoods>
    </MCMC>
    <MC3 chains="8" class="mcmc.mc3.MC3" length="10000000"
        swap.steps="500" threads="4">
        <MCMC/>
        <ChainHeats class="mcmc.mc3.ExpChainHeats" numberOfChains="8">
            <Lambda class="parameter.DoubleParameter" frequency="1.0"
                lowerBound="1.0E-10" upperBound="0.5" value="0.001">
                <ChainHeatsModifier class="modifier.ScaleModifier" frequency="1.0"/>
            </Lambda>
        </ChainHeats>
        <MCListeners class="list">
            <StateLogger/>
        </MCListeners>
    </MC3>
</ACG>
